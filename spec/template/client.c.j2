{% extends "base/wrap.c.j2" %}
{% block headers %}
#include <stdio.h>
{{ super() }}
{% endblock %}
{% block call %}
    {% if func.args %}
    printf("{{ func.name }}({{ func.args|printf }});\n", {{ func.args|args(0) }});
    {% else %}
    printf("{{ func.name }}();\n");
    {% endif %}
    {% if not func.void %}
    return 0;
    {% endif %}
{% endblock %}
